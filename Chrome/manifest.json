{
  "name": "DesLoader for Desmos",
  "description": "DesLoader Adds all types of tools to load things into Desmos.",
  "manifest_version": 3,
  "version": "0.1.3",
  "icons": {
    "48": "DesLoaderIcon.png",
    "128": "DesLoaderIcon.png"
  },
  "action": {
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.desmos.com/calculator*",
        "*://*.desmos.com/geometry*",
        "*://*.desmos.com/3d*"
      ],
      "run_at": "document_start",
      "js": ["script.js"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["script.js","injected.js"],
      "matches": ["*://*.desmos.com/*"]
    }
  ]
}
