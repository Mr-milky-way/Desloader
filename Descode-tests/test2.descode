var number Angle = 0
var number Speed = 70

function Test() {
    if (Speed = 70) {
        if (Angle = 0) {
            if (Speed = 90) {
                Speed = 70;
                Angle = 10;
            } else if (Angle = 0) {
                Angle = 10;
            }
        } else if (Angle = 10) {
            if (Speed = 70) {
                Speed = 0;
            }
            Angle = 0;
        } else {
            Speed = 70;
        }
    }
}